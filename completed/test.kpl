PROGRAM TEST;
VAR SCORE: INTEGER;

PROCEDURE OUTPUT( S:Integer); 
  BEGIN
   IF S>8 THEN CALL WRITEC('A') 
   ELSE IF S>7 THEN CALL WRITEC('B')
   ELSE IF S>6 THEN CALL WRITEC('C')
   ELSE IF S>5 THEN CALL WRITEC('D')
   ELSE IF S>4 THEN CALL WRITEC('E')
   ELSE CALL WRITEC('F');   
  END;

BEGIN

 SCORE:=READI; 
 CALL OUTPUT(SCORE); 
  
END. 